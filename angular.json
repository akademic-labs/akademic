{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "atividades-complementares": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "prefix": "aka"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "apps/atividades-complementares",
      "sourceRoot": "apps/atividades-complementares/src",
      "prefix": "aka",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/atividades-complementares",
            "index": "apps/atividades-complementares/src/index.html",
            "main": "apps/atividades-complementares/src/main.ts",
            "polyfills": "apps/atividades-complementares/src/polyfills.ts",
            "tsConfig": "apps/atividades-complementares/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/atividades-complementares/src/favicon.ico",
              "apps/atividades-complementares/src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "apps/atividades-complementares/src/styles.scss",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/themes/nova/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "apps/atividades-complementares/src/assets/css/custom-primeng.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/bootstrap-notify/bootstrap-notify.min.js",
              "node_modules/chart.js/dist/chart.min.js",
              "apps/atividades-complementares/src/assets/js/palette.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/atividades-complementares/src/environments/environment.ts",
                  "with": "apps/atividades-complementares/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "atividades-complementares:build:production"
            },
            "development": {
              "browserTarget": "atividades-complementares:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "atividades-complementares:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/atividades-complementares/src/test.ts",
            "polyfills": "apps/atividades-complementares/src/polyfills.ts",
            "tsConfig": "apps/atividades-complementares/tsconfig.spec.json",
            "karmaConfig": "apps/atividades-complementares/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/atividades-complementares/src/favicon.ico",
              "apps/atividades-complementares/src/assets"
            ],
            "styles": [
              "apps/atividades-complementares/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {}
        }
      }
    }
  }
}
