{
  "projectType": "application",
  "root": "apps/angular/atividades-complementares",
  "sourceRoot": "apps/angular/atividades-complementares/src",
  "prefix": "aka",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/angular/atividades-complementares",
        "index": "apps/angular/atividades-complementares/src/index.html",
        "main": "apps/angular/atividades-complementares/src/main.ts",
        "polyfills": "apps/angular/atividades-complementares/src/polyfills.ts",
        "tsConfig": "apps/angular/atividades-complementares/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "statsJson": true,
        "assets": [
          "apps/angular/atividades-complementares/src/favicon.ico",
          "apps/angular/atividades-complementares/src/assets"
        ],
        "styles": [
          "node_modules/bootstrap/dist/css/bootstrap.min.css",
          "apps/angular/atividades-complementares/src/styles.scss",
          "node_modules/primeicons/primeicons.css",
          "node_modules/primeng/resources/themes/nova/theme.css",
          "node_modules/primeng/resources/primeng.min.css",
          "apps/angular/atividades-complementares/src/assets/css/custom-primeng.css"
        ],
        "scripts": [
          "node_modules/jquery/dist/jquery.min.js",
          "node_modules/bootstrap/dist/js/bootstrap.min.js",
          "node_modules/bootstrap-notify/bootstrap-notify.min.js",
          "node_modules/chart.js/dist/chart.min.js",
          "apps/angular/atividades-complementares/src/assets/js/palette.js"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/angular/atividades-complementares/src/environments/environment.ts",
              "with": "apps/angular/atividades-complementares/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "atividades-complementares:build:production"
        },
        "development": {
          "browserTarget": "atividades-complementares:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "atividades-complementares:build"
      }
    },
    "deploy": {
      "executor": "@angular/fire:deploy",
      "options": {}
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/angular/atividades-complementares"],
      "options": {
        "jestConfig": "apps/angular/atividades-complementares/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/angular/atividades-complementares/src/**/*.ts",
          "apps/angular/atividades-complementares/src/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "bundle-analyzer": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npx webpack-bundle-analyzer dist/apps/angular/atividades-complementares/stats.json -m static -r dist/workspace/bundle-analyzer/atividades-complementares/index.html -t 'Atividades Complementares Webpack Bundle Analyzer' -s gzip -O"
      }
    }
  }
}
